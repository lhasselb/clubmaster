{
    "name": "lhasselb/clubmaster",
    "description": "Club management module.",
    "type": "silverstripe-vendormodule",
    "homepage": "https://github.com/lhasselb/clubmaster",
    "keywords": [
        "silverstripe",
        "club management"
    ],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "lhasselb",
            "email": "guggelimehl@gmail.com"
        }
    ],
    "require": {
        "silverstripe/framework": "^4.0",
        "silverstripe/admin": "^1.0",
        "globalcitizen/php-iban": "^2.6",
        "dynamic/silverstripe-country-dropdown-field": "^1.2",
        "colymba/gridfield-bulk-editing-tools": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Sybeha\\Clubmaster\\": "src/",
            "Sybeha\\Clubmaster\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.x-dev"
        },
        "expose": [
            "client/dist",
            "javascript",
            "css"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
